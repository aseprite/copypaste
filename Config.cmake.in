@PACKAGE_INIT@

set_and_check(@PROJECT_NAME@_INCLUDE_DIR "@PACKAGE_INSTALL_INC_DIR@")
set_and_check(@PROJECT_NAME@_INCLUDE_DIRS "@PACKAGE_INSTALL_INC_DIR@")

set(COMPAT_INC_DIR "@PACKAGE_COMPAT_INC_DIR@")

if(COMPAT_INC_DIR)
   list(APPEND @PROJECT_NAME@_INCLUDE_DIRS "@PACKAGE_COMPAT_INC_DIR@")
endif(COMPAT_INC_DIR)

set(@PROJECT_NAME@_FOUND TRUE)

include("${CMAKE_CURRENT_LIST_DIR}/@TARGETS_EXPORT_NAME@.cmake")
check_required_components("@PROJECT_NAME@")

string(TOUPPER @PROJECT_NAME@ UPPER_PROJECT_NAME)
set(${UPPER_PROJECT_NAME}_INCLUDE_DIR ${@PROJECT_NAME@_INCLUDE_DIR})
set(${UPPER_PROJECT_NAME}_INCLUDE_DIRS ${@PROJECT_NAME@_INCLUDE_DIRS})
set(${UPPER_PROJECT_NAME}_FOUND ${@PROJECT_NAME@_FOUND})
